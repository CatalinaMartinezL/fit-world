{
  "name": "nuevo-proyecto",
  "version": "1.0.0",
  "description": "A Vue.js project for Fit World, a fitness class platform.",
  "main": "babel.config.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.0",
    "@vue/compiler-sfc": "^3.2.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "warn",
      "vue/no-unused-components": "warn"
    }
  },
  "license": "MIT"
}
